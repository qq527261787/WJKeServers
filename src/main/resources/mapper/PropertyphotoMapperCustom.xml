<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.swjtu.WJKeServers.mapper.PropertyphotoMapperCustom">

  <select id="queryPhoto" resultType="com.swjtu.WJKeServers.entity.Propertyphoto" parameterType="String">
    select *
    from propertyphoto pp
    where pp.propertyid = #{propertyid}
  </select>

</mapper>